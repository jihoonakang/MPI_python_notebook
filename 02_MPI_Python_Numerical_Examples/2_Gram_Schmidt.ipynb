    "import numpy as np\n",
    "\n",
    "V = np.transpose(v).copy()\n",
    "for j in range (N) :\n",
    "    for i in range (j) :\n",
    "        coef = -np.dot(V[i], V[j])\n",
    "        V[j] = V[j] + coef * V[i]\n",
    "    coef = np.linalg.norm(V[j])\n",
    "    V[j] = V[j] / coef\n",
    "\n",
    "np.set_printoptions(linewidth=np.inf)\n",
    "print(\"V transpose : \")\n",
    "print(V)\n",
    "\n",
    "print(\"Norm ( V_T [0] ) : \")\n",
    "print(np.linalg.norm(np.transpose(V)[0]))"
   ]
